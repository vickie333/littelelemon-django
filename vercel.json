{
    "builds": [
        {
            "src": "littlelemon/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.13"
            }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "littlelemon/wsgi.py"
        }
    ]
}